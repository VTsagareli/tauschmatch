{
  "name": "tauschmatch",
  "version": "0.1.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean:scrapers": "rm -rf dist/scrapers",
    "build:scrapers": "tsc --project tsconfig.scrapers.json",
    "run:scraper": "node dist/scrapers/$npm_config_name.js",
    "build:scripts": "tsc --project tsconfig.json --outDir dist/scripts src/scripts/*.ts",
    "run:script": "tsx src/scripts/$npm_config_name.ts"
  },
  "dependencies": {
    "firebase": "^11.10.0",
    "firebase-admin": "^13.4.0",
    "next": "15.3.5",
    "openai": "^5.8.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@types/node": "^20.19.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-next": "15.3.5",
    "postcss": "^8.5.6",
    "puppeteer": "^24.12.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "tailwindcss": "^3.4.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3"
  }
}
